#### Overriding  for your IDE configuration, need EditorConfig plugin : http://editorconfig.org/#download

# top-most EditorConfig file
root = true

# Unix-style file
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
